.navdiv {
    width: 100%;
    padding-top:5px;
     padding-bottom:5px;
}
 
.navdiv ul {
    display: flex;
    flex-direction: row;
    margin: 0;
    padding: 0;
}
 
.navdiv ul li {
    list-style: none;
    flex-grow: 1;
    text-align: center;
}
 
.navdiv ul li a {
    display: block;
     font-family: Tahoma;
     color: #FFF;
     text-decoration: none;
}

