        .jumbotron {
    
    background: #ffffff url('../Images/slide_1.jpg') no-repeat center center fixed;
    background-size: cover;
    margin-bottom: 0px;
    min-height:500px;
    overflow: hidden;
    padding: 100px 0px;
}


.header_text {
            padding-left: 5px;
            font-family: Segoe UI;
            font-size: 20px;
            color: #162758;
            height: auto;
        }

.navbar 
{
    margin-bottom: 0px;
    min-height: 0px;
}
        

        .navbar-default {
            background-image: url('../Images/top_bg_blue.png')!important;
            background-repeat: repeat-x; /* new */
            border: 0px solid red;
            background-color: #fff;
            font-family: Segoe UI;
        }


            .navbar-default .navbar-brand {
                color: #172758;
            }

        .navbar-brand {
            font-family: 'Segoe UI';
        }

        .navbar-default .navbar-brand:focus, .navbar-default .navbar-brand:hover {
            color: #ffffff;
            background-color: transparent;
        }

        .navbar-default .navbar-nav > li > a {
            color: #172758;
        }

            .navbar-default .navbar-nav > li > a:focus, .navbar-default .navbar-nav > li > a:hover {
                color: #ffffff;
                background-color: transparent;
                background-color: #0b889e;
            }

        .nav .open > a, .nav .open > a:focus, .nav .open > a:hover {
            color: #FFF;
            background-color: #0B889E;
            border-color: #0B889E;
        }

        .nav > li > a:focus, .nav > li > a:hover {
            text-decoration: none;
            color: #FFF;
            background-color: #0B889E;
        }


        a {
            text-decoration: none;
            color: #0B889E;
        }

         .btn-primary {
            border-color: #2E6DA4;
            color: #182a38;
            background-color: #3ecae5;
        }

            .btn-primary.focus, .btn-primary:focus {
                border-color: #2E6DA4;
                color: #ffffff;
                background-color: #0b889e;
            }

        .btn.focus, .btn:focus, .btn:hover {
            color: #ffffff;
            background-color: #0b889e;
            text-decoration: none;
        }

        
        .row_links a {
            font-family: Tahoma;
            color: #FFF;
            text-decoration: none;
            
